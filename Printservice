class PrintService {
    // Method to print an integer
    void display(int data) {
        System.out.println("Integer data: " + data);
    }

    // Overloaded method to print a string
    void display(String data) {
        System.out.println("String data: " + data);
    }

    // Overloaded method to print two values
    void display(String data, int id) {
        System.out.println("ID: " + id + " Name: " + data);
    }

    public static void main(String[] args) {
        PrintService service = new PrintService();
        service.display(100);
        service.display("Hello");
        service.display("Admin", 1);
    }
}
