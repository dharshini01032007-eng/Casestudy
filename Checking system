import java.util.Scanner;

public class QualityCheckSystem {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Enter the batch number to check quality (Prime Check): ");
        int num = scanner.nextInt();
        
        boolean isPrime = (num > 1);

        for (int i = 2; i <= Math.sqrt(num); i++) {
            if (num % i == 0) {
                isPrime = false;
                break;
            }
        }

        if (isPrime) {
            System.out.println("Result: " + num + " passed the Quality Check (Prime).");
        } else {
            System.out.println("Result: " + num + " failed the Quality Check (Not Prime).");
        }
        scanner.close();
    }
}
